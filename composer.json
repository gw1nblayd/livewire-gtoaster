{
  "name": "gw1nblayd/livewire-gtoaster",
  "description": "Toasters for your Laravel Livewire applications",
  "license": "MIT",
  "authors": [
    {
      "name": "Kirill Zhelebovskiy",
      "email": "kirhoc0898@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "toaster",
    "toast",
    "livewire toaster",
    "laravel toaster",
    "alpine toaster",
    "alpine toast"
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "livewire/livewire": "^3.0|dev-main"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Gw1nblayd\\GToaster\\": "src/app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gw1nblayd\\GToaster\\Tests\\": "./tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gw1nblayd\\GToaster\\Providers\\LivewireGToasterProvider"
      ]
    }
  }
}
